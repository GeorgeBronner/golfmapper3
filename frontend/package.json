  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:local": "npx cross-env VITE_BACKEND_SERVER_IP=localhost VITE_BACKEND_PROTOCOL=http VITE_BACKEND_PORT=8005 vite build && npm run copy:dist",
    "build:prod": "npx cross-env VITE_BACKEND_SERVER_IP=golf.bronnerapp.com VITE_BACKEND_PROTOCOL=https VITE_BACKEND_PORT=443 vite build && npm run copy:dist",
    "copy:dist": "node -e \"require('fs').cpSync('dist', '../backend/dist', {recursive: true})\"",
